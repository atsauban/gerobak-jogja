# WhatsApp Configuration
# Ganti dengan nomor WhatsApp bisnis Anda (format: 62 + nomor tanpa 0)
# Contoh: 628123456789
VITE_WHATSAPP_NUMBER=6282327220077

# API Configuration
# Backend API URL (uncomment saat backend sudah ready)
# VITE_API_URL=http://localhost:5000/api
# VITE_API_URL=https://your-backend.com/api

# Firebase Configuration
# Dapatkan dari Firebase Console > Project Settings > General
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Cloudinary Configuration (untuk Netlify Functions)
# Dapatkan dari Cloudinary Console > Dashboard > API Keys
# PENTING: Jangan commit file .env ke Git!
CLOUDINARY_CLOUD_NAME=dpjpj7l1y
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret
